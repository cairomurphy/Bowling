@{
    ViewData["Title"] = "Home Page";
}

@model List<Bowler>

<h1>Contact List for Bowlers</h1>

<div id="teams" class="col-3">
    @*<vc:teams></vc:teams>*@
</div>

<div>
   
        <table class="table table-bordered table-dark table-striped">
            <thead class="font-weight-bold">
                <tr>
                    <td>FirstName</td>
                    <td>Middle Inital</td>
                    <td>Last Name</td>
                    <td>Address</td>
                    <td>City</td>
                    <td>State</td>
                    <td>Zip</td>
                    <td>Phone Number</td>
                </tr>
            </thead>
            
            <tbody>
                @foreach (var b in Model)
                {
                    <partial name="BowlersSummary" model="b" />

                }
            </tbody>
        </table>
  
</div>